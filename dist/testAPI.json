	"scripts": {
		"draw": "function draw(){\n    \n    var werk = window.chartWerk;\n    \n\tvar svg = d3.select(\"#chart\")\n\t    .append(\"svg\")\n\t    .attr({\n\t        \"width\": werk.ui.size === 'single' ? 290 : 600,\n\t        \"height\": 300\n\t    })\n\t    .style({\n\t        \"background\": \"pink\"\n\t    });\n\t\n\twerkHelper.dataX.map(function(d, i){\n\t    svg.append(\"rect\")\n\t        .attr({\n\t            x: d,\n\t            y: werkHelper.dataY[i],\n\t            width: 40,\n\t            height: 40\n\t        })\n\t        .style({\n\t            fill: \"rgb(110,140,250)\"\n\t        });\n\t})\n}",
		"helper": "var werkHelper = {\n    dataX: [0,60,120,180,240],\n    dataY: [0,40,80,120,160]\n};",
		"styles": "@import url(https://fonts.googleapis.com/css?family=Montserrat:400,700);\n\n#chartWerk {\n    font-family:'Montserrat', arial, sans-serif; \n}\n#chartWerk.single{\n    width:290px;\n    float:left;\n    overflow:hidden;\n    margin:10px 30px 10px 0;\n}\n#chartWerk #headline { \n    font: bold 28px 'Montserrat', arial;\n}\n#chartWerk #chatter {\n    font: normal 14px/17px 'Montserrat', arial;\n    margin-top:5px;\n}\n#chartWerk #footnote, #chartWerk #source {\n    font: normal 12px/15px arial, sans-serif;\n    color:grey;\n}\n#chartWerk #credit {\n    font: italic 14px/17px arial, sans-serif;\n    color:grey;\n}",
		"html": "<div id='chart-header'>\n\t<h2 id='headline'>Headline</h2>\n\t<div id='chatter'>Chat chat chatter.</div> \n</div> \n<div id='chart-ui'>\n\t<!--You can put any necessary buttons, etc., here.-->\n</div>\n<div id='chart-legend'></div>\n<div id='chart'></div> \n<div id='chart-footer'> \n\t<div id='footnote'>Footy footy footnote</div> \n\t<div id='source'>U.S. Department of Agriculture</div> \n\t<div id='author'>Jon McClure</div> \n</div>",
